<?xml version='1.0'?>
<a:skin xmlns:a="http://ajax.org/2005/aml">
    <a:window name="guidetool">
        <a:style><![CDATA[
            .guidetool {
                position : relative;
                overflow : visible;
                z-index  : 10000;
                box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.25);
                -moz-border-radius    : 8px;
                -webkit-border-radius : 8px;
                border-radius         : 8px;
            }
            .guidetool .gt-header {
                display : none;
            }
            .guidetool .gt-content {
                background : #222222;
                border : 1px solid #999;
                -moz-border-radius    : 8px;
                -webkit-border-radius : 8px;
                border-radius         : 8px;
                padding: 15px 15px;
            }
            .guidetool .gt-content .text {
                color: #cee57f;
                background-color: #222222;
            }
            .guidetool .gt-arrow {
                position: absolute;
                width : 16px;
                height : 16px;
                background : transparent;
            }
            .guidetool.top .gt-arrow {
                left: 50%;
                bottom: -9px;
                width: 0px;
                height: 0px;
                border-left: 10px solid transparent;
                border-right: 10px solid transparent;
                border-top: 10px solid #222;
            }
            .guidetool.bottom .gt-arrow {
                left: 50%;
                top: -9px;
                width: 0px;
                height: 0px;
                border-left: 10px solid transparent;
                border-right: 10px solid transparent;
                border-bottom: 10px solid #222;
            }
            .guidetool.left .gt-arrow {
                right: -9px;    
                top: 43%;
                width: 0px;
                height: 0px;
                border-top: 10px solid transparent;
                border-bottom: 10px solid transparent;
                border-left: 10px solid #222;
            }
            
            .guidetool.right .gt-arrow {
                left: -9px;	
                top: 43%;
                width: 0px;
                height: 0px;
                border-top: 10px solid transparent;
                border-bottom: 10px solid transparent;
                border-right: 10px solid #222;
            }
            
            .wn_cp_content .btnIcon .lbl span {
                width: 40px;
            }
             
            .tgDialog, .guidetool {
                z-index: 999999999999;
            }
             
            .tgDialog > .bk-header {
                padding-bottom: 10px;
            }
                
            .tgDesc {
            }
             
            .tgDesc a {
                color: #20800a;
                text-decoration: none;
            }

            .tgControlButton {
                width: 85px;
            }
            
            .tgPlayButton {
                width: 40px;
            }
            
            .tgPlayButton .caption {
                height: 15px;
                background: url(images/tg_button_play.png) no-repeat 14px 4px;
            }
            
            .tgBackButton .caption {
                height: 15px;
                background: url(images/tg_button_back.png) no-repeat 20px -21px;
            }
            
            .tgForwardButton .caption {
                height: 15px;
                background: url(images/tg_button_forward.png) no-repeat 24px 5px;
            }
        ]]></a:style>
    
        <a:presentation>
            <a:main
              collapsed-height = "30"
              minheight        = "30"
              minwidth         = "60"
              container        = "div[2]"
              drag             = "div[2]"
              title            = "div[1]/div[1]/text()"
              icon             = "div[1]/div[1]/div[1]"
              buttons          = "div[1]/div[1]/div[2]">
                <div class="guidetool">
                    <div class="gt-header">
                        <div class="hcontent">-<div class="icon"></div><div class="buttons"></div></div>
                        <div class="right"></div>
                    </div>
                    <div class="gt-content"> </div>
                    <div class="gt-arrow"></div>
                </div>
            </a:main>
            <a:button>
                <div> </div>
            </a:button>
        </a:presentation>
    </a:window>
    <a:modalwindow name="win-controls">
        <a:style><![CDATA[
            .win-controls {
                position : relative;
                overflow : hidden;
                z-index  : 10000;
                
                -moz-border-radius    : 6px 6px 6px 6px;
                -webkit-border-radius : 6px 6px 6px 6px;
                border-radius         : 6px 6px 6px 6px;
                
                -webkit-box-shadow: 0 2px 10px 1px #000000;
                -moz-box-shadow: 0 2px 10px 1px #000000;
                box-shadow: 0 2px 10px 1px #000000;
                border: 1px solid #000101;
                background: url(images/controls-bg.png) repeat-x 0 0;
            }

            .win-controls .wc-container {
                position:absolute;
                overflow:visible;
                left : 0;
                top  : 0;
                right : 0;
                bottom : 0;
                z-index: 1;
            }
        ]]></a:style>
    
        <a:presentation>
            <a:main
              collapsed-height = "26"
              minheight        = "50"
              minwidth         = "50"
              container        = "div[1]"
              title            = "div[1]/text()">
                <div class="win-controls">
                    <div class="wc-container"> </div>
                </div>
            </a:main>
            <a:button>
                <div/>
            </a:button>
        </a:presentation>
    </a:modalwindow>
</a:skin>