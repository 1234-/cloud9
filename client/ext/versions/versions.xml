<a:application xmlns:a="http://ajax.org/2005/aml">
    <a:style><![CDATA[
        .historical_pages {
            background: #666;
            border: 1px solid #999;
            -webkit-border-top-left-radius: 5px;
            -webkit-border-top-right-radius: 5px;
            -moz-border-radius-topleft: 5px;
            -moz-border-radius-topright: 5px;
            border-top-left-radius: 5px;
            border-top-right-radius: 5px;
            -webkit-box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.75);
            -moz-box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.75);
        }

        .versions_label {
            border-radius: 5px;
            -moz-border-radius: 5px;
            -webkit-border-radius: 5px;
            border: 1px solid #999;
            background: #222;
            color: #aaa;
            padding: 5px 12px;
            font-size: 13px;
            opacity: 0.9;
        }

        .versions_search {
            border-radius: 15px;
            -moz-border-radius: 15px;
            -webkit-border-radius: 15px;
            border: 1px solid #999;
            background: #222;
        }

        .versions_search .sbtb_middle {
            background: none;
            border: none;
            -webkit-box-shadow: none;
            -moz-box-shadow: none;
            box-shadow: none;
        }

        .versions_search .sbtb_middle input {
            color: #ddd;
        }

        .versions_navigate {
            border-radius: 5px;
            -moz-border-radius: 5px;
            -webkit-border-radius: 5px;
            background: #222;
            border: 1px solid #999;
            height: 27px;
            width: 15px;
            opacity: 0.9;
        }

        .versions_navigate .left, .versions_navigate .right, .versions_navigate .lbl {
            background: none;
        }

        .versions_navigate.btnOver {
            background: #2f2f2f;
        }

        .versions_navigate.nav_backward {
            right: 10px;
            background-image: url('/static/style/images/versions_left_arrow.png');
            background-position: 6px 5px;
            background-repeat: no-repeat;
        }

        .versions_navigate.nav_forward {
            left: 10px;
            background-image: url('/static/style/images/versions_right_arrow.png');
            background-position: 8px 5px;
            background-repeat: no-repeat;
        }

        #versionsHistoricalSlider {
            width: 100%;
            height: 2px;
            margin-top: 20px;
            background: #fff;
            position: relative;
            border: 1px solid #333;
        }

        #versionsHistoricalSlider u {
            position: absolute;
            top: -3px;
            height: 8px;
            width: 8px;
            border-radius: 5px;
            -moz-border-radius: 5px;
            -webkit-border-radius: 5px;
            border: 1px solid #333;
            background: #fff;
            cursor: pointer;
            margin-left: 0px;
            z-index: 0;
            opacity: 1.0;
        }

        #versionsHistoricalSlider u.pop {
            -webkit-transform: scale(2.5);
            -moz-transform: scale(2.5);
            -o-transform: scale(2.5);
            -ms-transform: scale(2.5);
            transform: scale(2.5);
            background: none;
            border: 1px solid #6C99BB;
            -webkit-transition-property: opacity;
            -webkit-transition-duration: 0.5s;
            opacity: 0.0;
        }

        #versionsHistoricalSlider u:hover, #versionsHistoricalSlider u.current {
            border: 1px solid green;
            top: -4px;
            height: 10px;
            width: 10px;
            margin-left: -1px;
            z-index: 10;
            -webkit-box-shadow: 0px 0px 5px rgba(0, 0, 0, 1);
            -moz-box-shadow: 0px 0px 5px rgba(0, 0, 0, 1);
            box-shadow: 0px 0px 5px rgba(0, 0, 0, 1);
        }
    ]]></a:style>
</a:application>