<?xml version='1.0'?>
<a:skin xmlns:a="http://ajax.org/2005/aml" xmlns="http://www.w3.org/1999/xhtml">
    <a:list name="issueslist">
        <a:style><![CDATA[
            .issueslist{
                overflow: auto;
                position: relative;
                background-color: #fff;
                color: #000;
                margin: 0;
                outline: none;
                font-family: Tahoma, Arial;
                font-size: 12px;
                cursor: default;
            }
            .issueslist > div:hover, .issueslist > div.odd:hover {
                background-color: #ffc; /*#e7e7e7;*/
            }
            .issueslist > .selected{
                background-color: #e0e0e0;
            }
            .issueslistFocus > .selected{
                
            }
            .issueslist .empty, .issueslist .offline, .issueslist .loading{
                text-align: center;
                padding: 8px 0 0 0;
                color: #777;
                font-size : 8pt;
                font-weight : normal;
            }
            .issueslist > div {
                padding: 8px 12px;
                line-height: 16px;
                border-bottom: 1px solid #ddd;
            }
            /* deprecated unless we can get logic working (see issuesmgr.xml)
            .issueslist > div.odd {
                background: #f9f9f9;
                border-top: 1px solid #ddd;
                border-bottom: 1px solid #ddd;
            }*/
            .issueslist .issue_top .number {
                color: #666;
                float: left;
            }
            .issueslist .issue_top .title {
                margin-left: 40px;
                font-weight: bold;
                word-wrap: break-word;
            }
            .issueslist .issue_bottom {
                font-size: 11px;
                margin-top: 4px;
                color: #666;
                position: relative;
                height: 16px;
            }
            .issueslist .issue_bottom p {
                position: absolute;
            }
            .issueslist .issue_bottom p.comments {
                right: 0px;
                padding-left: 20px;
                background: url(images/comments.png) 0px 0px no-repeat;
            }
            .issueslist .issue_bottom .user {
                color: #000;
            }
        ]]></a:style>

        <a:presentation>
            <a:main container=".">
                <div class="issueslist">
                </div>
            </a:main>
            <a:item class="." caption="." select="." container=".">
                <div>
                </div>
            </a:item>
            <a:empty caption=".">
                <div class="message">-</div>
            </a:empty>
        </a:presentation>
    </a:list>
    <a:tab name="issuestab">
        <a:style><![CDATA[
            .issuestab {
                padding : 0 0 0 0;
                border: 0px solid #c3c3c3;
                margin : 0 auto;
                overflow: hidden;
                position:relative;
                padding-top: 0;
                /*border-bottom: 1px solid white;*/
            }
    
            .issuestab .btncontainer{
                padding : 0 0 1px;
                z-index : 100;
                height: 15px;
                overflow: visible;
                white-space: nowrap;
                color: #404040;
                text-shadow : rgba(255, 255, 255, 1) 0px 1px 0px;
                font-family : Tahoma, Arial;
                font-size : 11px;
                border-bottom : 1px solid #8c8c8c;
    
                background: -webkit-gradient(linear,left bottom,left top,color-stop(0, rgb(209,209,209)),color-stop(1, rgb(227,227,227)));
                background: -moz-linear-gradient(center bottom,rgb(209,209,209) 0%,rgb(227,227,227) 100%);
                margin-top : -16px;
                width : auto;
                display : none;
            }
    
            .issuestab .btncontainer>div {
                display: block;
                display : inline-block;
                margin: 0 0 0 0;
                padding: 0;
                position : relative;
                vertical-align : bottom;
                cursor : default;
            }
    
            .issuestab .btncontainer .btntab {
                padding      : 1px 10px 2px 10px;
                position     : relative;
                margin-top:-1px;
                text-align: center;
                border-top : 1px solid #ffffff;
                border-right : 1px solid #8c8c8c;
                border-bottom : 1px solid #8c8c8c;
            }
    
            .issuestab .btncontainer .btntab .issuestab_icon {
                height:16px;
                left:1px;
                position:absolute;
                top:1px;
                width:16px;
            }
    
            .issuestab .btncontainer .btntab .issuestab_title {
                overflow : hidden;
                padding  : 0 0 0 0;
            }
    
            .issuestab .btncontainer .btntab.btnclose .issuestab_title {
                margin-right : 15px;
            }
    
            .issuestab .scale .btntab .issuestab_title{
                display : block;
                overflow : hidden;
                text-overflow : ellipsis;
                white-space : nowrap;
            }
            .issuestab .btncontainer div.curbtn {
                color       : #202020;
                cursor      : default;
                z-index     : 1;
                border-right : 1px solid #8c8c8c;
                border-top: 1px solid #f9f9f9;
                border-bottom: 1px solid #F6F6F6;
            }
    
            .issuestab .scale .btntab .issuestab_middle {
                display : block;
                overflow : hidden;
                text-overflow : ellipsis;
                white-space : nowrap;
            }
            .issuestab .btncontainer .btnclose strong {
                width               : 14px;
                height              : 14px;
                display             : block;
                position            : absolute;
                right               : 0;
                top                 : 7px;
                background-image    : url(images/close_tab_btn.png);
                background-repeat   : no-repeat;
                background-position : 0 -14px;
            }
    
            .issuestab .btncontainer .btnclose strong:hover {
                background-position : -14px -14px;
            }
    
            .issuestab .btncontainer .btnclose strong:active {
                background-position : -28px -14px;
            }
    
            .issuestab .btncontainer .curbtn.btnclose strong {
                background-position : 0 0;
                top: 7px;
            }
    
            .issuestab .btncontainer .curbtn.btnclose strong:hover {
                background-position : -14px 0;
            }
    
            .issuestab .btncontainer .curbtn.btnclose strong:active {
                background-position : -28px 0;
            }

            .issuestab .page.curpage{
                display : block;
            }
    
            .issuestab .page {
                display : none;
                position : absolute;
                top : 0;
                bottom : 0;
                left : 0;
                right : 0;
                z-index : 10;
                background: #f6f6f6;
                color: #222;
                clear: both;
                font-family : Arial;
                font-family: 12px;
                overflow: auto;
            }
            
            .issuestab .page.issue_detail {
                padding: 10px 15px 30px 15px;
            }

            .issuestab .page.issue_detail .labels {
                padding: 10px 0 0 0;
            }

            .issuestab .page.issue_detail .issue_label {
                float: left;
                padding: 3px 12px;
                background: #eee;
                color: #333;
                border: 1px solid #bbb;
                border-radius: 3px;
                margin: 10px 5px 0 0;
            }
            .issuestab .page p {
                margin: 0;
                word-wrap: break-word;
            }

            .issuestab .page .issue_header img {
                float: left;
            }

            .issuestab .page .issue_header h1 {
                font-size: 16px;
                margin: 0 0 0 45px;
            }

            .issuestab .page p.author {
                color: #666;
                line-height: 16px;
            }
            
            .issuestab .page a.author_link {
                font-weight: bold;
            }
            
            .issuestab .page p a {
                text-decoration: none;
                color: #444;
            }
            
            .issuestab .page p a:visited {
                color: #666;
            }
            
            .issuestab .page p a:hover {
                text-decoration: underline;
            }

            .issuestab .page p.issue_body {
                clear: left;
                padding : 15px 0 10px 0;
                font-size: 12px;
                line-height: 16px;
                font-family: Tahoma, Arial;
            }
            
            .issue_comments_list .comment {
                margin-top: 20px;
            }

            .issue_comments_list .comment p {
                font-size: 12px;
                line-height: 16px;
                font-family: Tahoma, Arial;
            }

            .issue_comments_divider {
                width: 265px;
                margin: 0 auto 0 auto;
                clear: left;
                padding: 20px 0 10px;
            }
            
            .comment_user {
                float: left;
            }
            
            .comment_user .user_image, .issuestab .page .issue_header img {
                box-shadow: inset 0px 0px 1px rgba(0, 0, 0, 0.5);
                border-radius: 4px;
                padding: 2px;
            }

            /*.issue_comments_list .comment_details h2 {
                margin: 0 0 10px 0;
                font-size: 14px;
                padding: 0;
            }*/

            .issue_comments_list .comment_details {
                margin-left: 40px;
                padding: 0px 10px;
                /*border: 1px solid #aaa;
                box-shadow: 0px 0px 3px rgba(0, 0, 0, 0.2);
                border-radius: 3px;
                */
            }
            
            .issue_comments_list .comment_details p.created_at {
                padding-top: 10px;
                color: #999;
                font-size: 11px;
            }
        ]]></a:style>
    
        <a:presentation>
            <a:main pages="." buttons="div[1]">
                <div class="issuestab">
                    <div class="btncontainer"> </div>
                </div>
            </a:main>
            <a:button caption="div[1]/text()" container="div" maxwidth="90" minwidth="10">
                <div class="btntab">
                    <!--div class="tab_icon"> </div-->
                    <div class="tab_title">-</div>
                </div>
            </a:button>
            <a:btnclose>
                <strong> </strong>
            </a:btnclose>
            <a:page container=".">
                <div class="page"> </div>
            </a:page>
        </a:presentation>
    </a:tab>
    <a:modalwindow name="issue-window">
        <a:style><![CDATA[
            .issue-window {
                position : relative;
                overflow : hidden;
        		z-index	 : 10000;
                padding  : 30px 0 0 0;
            }
            
            .issue-window .fm-header {
                position : relative;
                overflow : hidden;
                height   : 30px;
                top      : -30px;
            }
            
            .issue-window .fm-header .hcontent {
                background    : url(images/header_backg.png)  repeat-x 0 0;
                margin-right  : 6px;
                position      : relative;
                overflow      : hidden;
                height        : 24px;
                padding       : 6px 0 0 6px;
                font-family : Tahoma, Arial;
                font-size   : 12px;
                line-height:16px;
                font-weight : bold;
                color       : #1d4566;
                text-shadow   : rgba(255, 255, 255, 0.25) 0px 1px 0px;
            }
            
            .issue-window .fm-header .right {
                width:6px;
                height:30px;
                position:absolute;
                top:0;
                right:0;
                background:url(images/header_right.png) no-repeat 0 0;
            }
    
            .issue-window .fm-content {
                position:absolute;
                overflow:visible;
                border-right:1px solid #7b7b7b;
                left : 0;
                top  : 30px;
                right : 0;
                bottom : 0;
            }
        ]]></a:style>
        
        <a:presentation>
            <a:main 
              collapsed-height = "30"
              minheight        = "30"
              minwidth         = "60"
              container        = "div[2]" 
              drag             = "div[2]" 
              title            = "div[1]/div[1]/text()" 
              icon             = "div[1]/div[1]/div[1]"
              buttons          = "div[1]/div[1]/div[2]">
                <div class="issue-window">
                    <div class="fm-header">
                        <div class="hcontent">-<div class="icon"></div><div class="buttons"></div></div>
                        <div class="right"></div>
                    </div>
                    <div class="fm-content"> </div>
                </div>
            </a:main>
            <a:button>
                <div> </div>
            </a:button>
        </a:presentation>
    </a:modalwindow>
</a:skin>
