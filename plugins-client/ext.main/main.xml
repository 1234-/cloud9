<a:application xmlns:a="http://ajax.org/2005/aml">
    <a:appsettings name="ide" debug="false"
      disable-space             = "true"
      auto-hide-loading         = "true"
      allow-select              = "false"
      allow-blur                = "false"
      initdelay                 = "false"
      autoHideLoading           = "false"
      storage                   = "cookie"
      baseurl                   = "{apf.host ? apf.host : ''}"
      requested-with-getparam   = "xhr" />

    <!-- default model -->
    <a:model />

    <a:state id="stServerConnected" active="false" />
    <a:state id="stProcessRunning" active="false" />

    <a:scrollbar
        id       = "sbShared"
        skin     = "sbios"
        top      = "0"
        right    = "0"
        bottom   = "0"
        width    = "7"
        showonscroll = "true"
        zindex   = "100000"
    />

    <a:bar anchors="0 0 0 0" id="vbMain">
        <a:bar skin="c9-menu-bar" id="logobar">
            <a:bar class="fakehbox aligncenter padding3" id="barTools">
                <a:divider skin="c9-divider-double" />
            </a:bar>
            <a:bar style="float:right;" class="fakehbox aligncenter padding3" id="barExtras">
                <a:divider skin="c9-divider" visible="false" />
            </a:bar>
        </a:bar>
        
        <a:bar class="fakehbox" anchors="27 0 0 0">
            <a:bar id="mainRow" anchors="0 0 0 0">
                <a:bar class="fakehbox" id="hboxTabBar" width="100%">
                    <a:hbox id="barButtonContainer" flex="1" align="start" />
                </a:bar>
                <a:bar class="fakehbox" id="hboxMain" padding="0" edge="0" flex="1" height="100%">
                    <a:bar id="colLeft" width="200" minwidth="105" visible="false"  height="100%"/>
                    <a:splitter width="0" id="splitterPanelLeft"  height="100%"/>
                    <a:bar id="colMiddle" flex="1" padding="3"  height="100%"/>
                </a:bar>
            </a:bar>
            <a:bar id="colRight" padding="0">
                <a:bar class="fakehbox hboxdockpanel" id="hboxDockPanel" flex="1">
                </a:bar>
            </a:bar>
        </a:bar>
    </a:bar>
</a:application>